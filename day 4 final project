pragma solidity ^0.4.17;

contract Reportcard{
    string public name;
    string public  rollno;
    uint batch;
    uint mathmarks=45;
    uint sciencemarks=35;
    uint socialmarks=48;
    function Reportcard(string newname,string newrollno ,uint newbatch )public{
   name=newname;
   rollno=newrollno;
   batch=newbatch;
}function setreportdetails(string newname,string newrollno ,uint newbatch )public{
   name=newname;
   rollno=newrollno;
   batch=newbatch;
}

function getmarks()public view returns(uint,uint,uint){
    return(mathmarks,sciencemarks,socialmarks);
 
}
   
}






